import java.util.Scanner;
public class Solution 
{
	public static void main(String[] args) 
  {
	      Scanner s=new Scanner(System.in);
        int n=s.nextInt();
     
        int i=1;
        while(i<=n)
        {  
        int j=1,counter=i;
        while(j<=i)
        {
        System.out.print(counter);
        counter=i-j;
        j++;
        }
        System.out.println();
	      i++;	
	      }
    }
}
