import java.util.Scanner;
public class Solution 
{
	public static void printSubstrings(String str) 
  {
        int n=str.length();
        for(int i=0;i<n;i++)
        {
            for(int j=0;j<=i;j++)
            {
                for(int k=j;k<=i;k++)
                {
                    System.out.print(str.charAt(k));
                }
                System.out.println("");
            }
        }
	}
}
