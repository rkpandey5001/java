import java.util.Scanner;
public class Solution 
{
	public static String reverseWordWise(String input)
  {
        String s[] = input.split(" ");
        String ans = "";
        for (int i = s.length - 1; i >= 0; i--)
        {
            ans += s[i] + " ";
        }
        return ans.substring(0,ans.length() - 1);
	}
}
