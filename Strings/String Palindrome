import java.util.Scanner;
public class Solution 
{
	public static boolean isPalindrome(String str) 
  {
		int size=str.length();
        int i=0;
        int j=size-1;
        while(i<j)
        {
            if(str.charAt(i)!=str.charAt(j))
            {
               return false;
            }
            i++;
            j--;
        }
    return true;
	}
}
