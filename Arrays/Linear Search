import java.util.Scanner;
public class Solution 
{
    public static int linearSearch(int arr[], int x) 
    {
		int i=0;
        int n=arr.length;
        while(i<n)
        {
            if(arr[i]==x)
            {
            return i;
            }
            i++;
        }
        return -1;
    }
}
