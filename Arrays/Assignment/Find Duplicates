import java.util.Scanner;
public class Solution
{    
    public static int duplicateNumber(int arr[]) 
    {
        int a=0;
		    for(int i=0;i<arr.length;i++)
        {
            for(int j=i+1;j<arr.length;j++)
            {
                if(arr[i]==arr[j]){
                a=arr[i];
                break;
                }
            }
        }
       return a;
       }
}
    
      
